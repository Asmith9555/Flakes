''
  # Main background, empty for terminal default, need to be empty if you want transparent background
  theme[main_bg]="#FAF4ED"

  # Main text color
  theme[main_fg]="#575279"

  # Title color for boxes
  theme[title]="#FFFAF3"

  # Higlight color for keyboard shortcuts
  theme[hi_fg]="#CECACD"

  # Background color of selected item in processes box
  theme[selected_bg]="#F2E9E1"

  # Foreground color of selected item in processes box
  theme[selected_fg]="#ECEFF4"

  # Color of inactive/disabled text
  theme[inactive_fg]="#F2E9E1"

  # Misc colors for processes box including mini cpu graphs, details memory graph and details status text
  theme[proc_misc]="#CECACD"

  # Cpu box outline color
  theme[cpu_box]="#F2E9E1"

  # Memory/disks box outline color
  theme[mem_box]="#F2E9E1"

  # Net up/down box outline color
  theme[net_box]="#F2E9E1"

  # Processes box outline color
  theme[proc_box]="#F2E9E1"

  # Box divider line and small boxes line color
  theme[div_line]="#F2E9E1"

  # Temperature graph colors
  theme[temp_start]="#F4EDe8"
  theme[temp_mid]="#DFDAD9"
  theme[temp_end]="#907AA9"

  # CPU graph colors
  theme[cpu_start]="#F4EDe8"
  theme[cpu_mid]="#DFDAD9"
  theme[cpu_end]="#907AA9"

  # Mem/Disk free meter
  theme[free_start]="#F4EDe8"
  theme[free_mid]="#DFDAD9"
  theme[free_end]="#907AA9"

  # Mem/Disk cached meter
  theme[cached_start]="#F4EDe8"
  theme[cached_mid]="#DFDAD9"
  theme[cached_end]="#907AA9"

  # Mem/Disk available meter
  theme[available_start]="#F4EDe8"
  theme[available_mid]="#DFDAD9"
  theme[available_end]="#907AA9"

  # Mem/Disk used meter
  theme[used_start]="#F4EDe8"
  theme[used_mid]="#DFDAD9"
  theme[used_end]="#907AA9"

  # Download graph colors
  theme[download_start]="#F4EDe8"
  theme[download_mid]="#DFDAD9"
  theme[download_end]="#907AA9"

  # Upload graph colors
  theme[upload_start]="#F4EDe8"
  theme[upload_mid]="#DFDAD9"
  theme[upload_end]="#907AA9"
